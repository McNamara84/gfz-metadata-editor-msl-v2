{
    "name": "elmo/elmo",
    "description": "EPOS Laboratory Metadata Optimizer. Editor for research metadata of MSL data.",
    "license": "GPL-2.0-only",
    "require": {
        "php": "^8.2 || ^8.3 || ^8.4",
        "easyrdf/easyrdf": "^1.1",
        "phpmailer/phpmailer": "^6.9",
        "nikic/fast-route": "^1.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.4",
        "guzzlehttp/guzzle": "^7.9"
    },
    "autoload": {
        "classmap": [
            "."
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "scripts": {
        "test": "phpunit"
    }
}